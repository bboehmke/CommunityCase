<idea-plugin>
  <name>Git Integration</name>
  <id>Git4Idea</id>
  <description>Provides integration with Git version control system</description>
  <version>8.1</version>
  <category>VCS Integration</category>
  <vendor url="http://svn.jetbrains.org/idea/Trunk/bundled/git4idea/" logo="/general/ijLogo.png">JetBrains</vendor>

  <depends>com.intellij.modules.vcs</depends>

  <actions>

    <action id="Git.Init" class="org.community.intellij.plugins.communitycase.actions.Init" text="Create Git Repository...">
      <add-to-group group-id="Vcs.Import"/>
    </action>

    <group id="Git.Menu" class="org.community.intellij.plugins.communitycase.actions.Menu" popup="true" text="_Git">
      <reference ref="CheckinFiles"/>
      <!-- <reference id="CheckinProject"/>
   <reference id="UpdateFiles"/>
   <reference id="Compare.SameVersion"/>
   <reference id="IntegrateFiles"/>  -->
      <reference ref="Annotate"/>
      <reference ref="Compare.SameVersion"/>
      <reference ref="Compare.LastVersion"/>
      <reference ref="Compare.Selected"/>
      <reference ref="Vcs.ShowTabbedFileHistory"/>
      <reference id="Vcs.ShowHistoryForBlock"/>
      <action id="Git.CompareWithBranch" class="org.community.intellij.plugins.communitycase.actions.CompareWithBranchAction" text="Compare with branch" />
      <!-- <reference id="ChangesView.Browse"/> -->
      <separator/>

      <action id="Git.CurrentBranch" class="org.community.intellij.plugins.communitycase.actions.CurrentBranch" text="Current Branch..."/>
      <action id="Git.Checkout" class="org.community.intellij.plugins.communitycase.actions.Checkout" text="Checkout Branch..."/>
      <action id="Git.Add" class="org.community.intellij.plugins.communitycase.actions.Add" text="Add" icon="/actions/include.png">
        <keyboard-shortcut first-keystroke="control alt A" keymap="$default"/>
      </action>
      <action id="Git.Revert" class="org.community.intellij.plugins.communitycase.actions.Revert" text="Revert File" icon="/actions/org.community.intellij.plugins.communitycase.rollback.png"/>
      <!--<action id="Resolve" class="org.jetbrains.idea.perforce.actions.ResolveAction"/>-->
      <!--<action id="ResolveAll" class="org.jetbrains.idea.perforce.actions.ResolveAllAction"/>-->
      <!-- <action id="TimeLapseView" class="org.jetbrains.idea.perforce.actions.TimeLapseViewAction"/> -->
      <separator/>

      <action id="Git.Tag" class="org.community.intellij.plugins.communitycase.actions.Tag" text="Tag Files..."/>
      <action id="Git.Merge" class="org.community.intellij.plugins.communitycase.actions.Merge" text="Merge Changes..." icon="/vcs/mergeSourcesTree.png"/>
      <action id="Git.MergeTool" class="org.community.intellij.plugins.communitycase.actions.MergeTool" text="Merge Tool..."/>
      <action id="Git.Stash" class="org.community.intellij.plugins.communitycase.actions.Stash" text="Stash Changes..."/>
      <action id="Git.Unstash" class="org.community.intellij.plugins.communitycase.actions.Unstash" text="UnStash Changes..."/>
      <action id="Git.Reset" class="org.community.intellij.plugins.communitycase.actions.ResetHead" text="Reset HEAD..." icon="/actions/reset.png"/>
      <separator/>
      <action id="Git.Fetch" class="org.community.intellij.plugins.communitycase.actions.Fetch" text="Fetch Changes..."/>
      <action id="Git.Pull" class="org.community.intellij.plugins.communitycase.actions.Pull" text="Pull Changes..." icon="/actions/checkOut.png"/>
      <action id="Git.Push" class="org.community.intellij.plugins.communitycase.actions.Push" text="Push Changes..." icon="/actions/commit.png"/>
      <action id="Git.Push.Active.Branches" class="org.community.intellij.plugins.communitycase.actions.PushActiveBranches" text="Push Active Branches..."/>
      <separator/>
      <action id="Git.Rebase" class="org.community.intellij.plugins.communitycase.actions.Rebase" text="Rebase..."/>
      <action id="Git.Rebase.Abort" class="org.community.intellij.plugins.communitycase.actions.RebaseAbort" text="Abort Rebasing"/>
      <action id="Git.Rebase.Continue" class="org.community.intellij.plugins.communitycase.actions.RebaseContinue" text="Continue Rebasing"/>
      <action id="Git.Rebase.Skip" class="org.community.intellij.plugins.communitycase.actions.RebaseSkip" text="Skip Commit in Rebasing"/>
      <separator/>

      <add-to-group group-id="VcsGroup" anchor="last"/>
      <add-to-group group-id="VcsGlobalGroup" anchor="after" relative-to-action="Vcs.Specific"/>
      <!--           <add-to-group anchor="last" group-id="VcsGlobalGroup" /> -->
    </group>
  </actions>

  <extensions defaultExtensionNs="com.intellij">
    <errorHandler implementation="com.intellij.diagnostic.ITNReporter"/>
    <vcs name="Git" vcsClass="git4idea.GitVcs" displayName="Git" administrativeAreaName=".git"/>
    <checkoutProvider implementation="org.community.intellij.plugins.communitycase.checkout.CheckoutProvider"/>
    <vcsPopupProvider implementation="org.community.intellij.plugins.communitycase.actions.QuickListContentProvider"/>

    <projectService
      serviceInterface="git4idea.org.community.intellij.plugins.communitycase.history.wholeTree.LogFactoryService"
      serviceImplementation="git4idea.org.community.intellij.plugins.communitycase.history.wholeTree.LogFactoryService"/>
    <projectService
        serviceInterface="git4idea.org.community.intellij.plugins.communitycase.annotate.GitAnnotationProvider"
        serviceImplementation="git4idea.org.community.intellij.plugins.communitycase.annotate.GitAnnotationProvider"/>
    <projectService
        serviceInterface="git4idea.diff.GitDiffProvider"
        serviceImplementation="git4idea.diff.GitDiffProvider"/>
    <projectService
        serviceInterface="git4idea.org.community.intellij.plugins.communitycase.checkin.CheckinEnvironment"
        serviceImplementation="git4idea.org.community.intellij.plugins.communitycase.checkin.CheckinEnvironment"/>
    <projectService
        serviceInterface="git4idea.org.community.intellij.plugins.communitycase.changes.GitChangeProvider"
        serviceImplementation="git4idea.org.community.intellij.plugins.communitycase.changes.GitChangeProvider"/>
    <projectService
        serviceInterface="git4idea.org.community.intellij.plugins.communitycase.history.HistoryProvider"
        serviceImplementation="git4idea.org.community.intellij.plugins.communitycase.history.HistoryProvider"/>
    <projectService
        serviceInterface="git4idea.org.community.intellij.plugins.communitycase.rollback.RollbackEnvironment"
        serviceImplementation="git4idea.org.community.intellij.plugins.communitycase.rollback.RollbackEnvironment"/>
    <projectService
        serviceInterface="git4idea.org.community.intellij.plugins.communitycase.config.VcsSettings"
        serviceImplementation="git4idea.org.community.intellij.plugins.communitycase.config.VcsSettings"/>
    <projectService
        serviceInterface="git4idea.org.community.intellij.plugins.communitycase.history.NewUsersComponent"
        serviceImplementation="git4idea.org.community.intellij.plugins.communitycase.history.NewUsersComponent"/>
    <projectService
        serviceInterface="git4idea.org.community.intellij.plugins.communitycase.history.browser.ProjectLogManager"
        serviceImplementation="git4idea.org.community.intellij.plugins.communitycase.history.browser.ProjectLogManager"/>
    <projectService
        serviceInterface="git4idea.org.community.intellij.plugins.communitycase.checkout.branches.BranchConfigurations"
        serviceImplementation="git4idea.org.community.intellij.plugins.communitycase.checkout.branches.BranchConfigurations"/>
    <projectService
        serviceInterface="git4idea.org.community.intellij.plugins.communitycase.config.ExecutableValidator"
        serviceImplementation="git4idea.org.community.intellij.plugins.communitycase.config.ExecutableValidator"/>
    <applicationService
        serviceInterface="git4idea.org.community.intellij.plugins.communitycase.config.VcsApplicationSettings"
        serviceImplementation="git4idea.org.community.intellij.plugins.communitycase.config.VcsApplicationSettings"/>
    <applicationService
        serviceInterface="git4idea.org.community.intellij.plugins.communitycase.commands.SshIdeaService"
        serviceImplementation="git4idea.org.community.intellij.plugins.communitycase.commands.SshIdeaService"/>
    <applicationService
        serviceInterface="git4idea.org.community.intellij.plugins.communitycase.rebase.RebaseEditorService"
        serviceImplementation="git4idea.org.community.intellij.plugins.communitycase.rebase.RebaseEditorService"/>
    <applicationService
        serviceInterface="git4idea.org.community.intellij.plugins.communitycase.config.SshConnectionSettings"
        serviceImplementation="git4idea.org.community.intellij.plugins.communitycase.config.SshConnectionSettings"/>

    <ComponentRoamingType component="Git.Settings" type="DISABLED"/>
    <fileTypeFactory implementation="git4idea.vfs.GitFileTypeFactory"/>
  </extensions>
</idea-plugin>
